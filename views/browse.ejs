<%- include("partials/navbar"); -%>

<div class="row">
    <% posts.forEach(function(post) { %>
        <div class="columns col-lg-2 col-md-4 col-sm-6">
            <a href="/posts/<%=post.postID%>">
                <div class="card">
                    <div class="text">
                        <h1><%=post.date%></h1>
                    </div>
                </div>
            </a>
        </div>
    <% }) %>
</div>

<%- include("partials/footer"); -%>